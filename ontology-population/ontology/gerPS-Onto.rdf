    <!-- https://w3id.org/GerPS-onto/ontology#hat_abgabefrist -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/GerPS-onto/ontology#hat_abgabefrist">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/GerPS-onto/ontology#hat_frist"/>
        <rdfs:domain rdf:resource="https://w3id.org/GerPS-onto/ontology#Dokument"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Submission deadlines are tied to documents and specify a time period in which a document must be submitted</rdfs:comment>
        <rdfs:label xml:lang="en">has_submission_deadline</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/GerPS-onto/ontology#hat_bearbeitungsfrist -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/GerPS-onto/ontology#hat_bearbeitungsfrist">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/GerPS-onto/ontology#hat_frist"/>
        <rdfs:domain rdf:resource="https://w3id.org/GerPS-onto/ontology#Prozess"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label xml:lang="en">has_processing_deadline</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/GerPS-onto/ontology#hat_datenfeldID -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/GerPS-onto/ontology#hat_datenfeldID">
        <rdfs:subPropertyOf rdf:resource="http://BPMNbasedOntology#id"/>
        <rdfs:domain rdf:resource="https://w3id.org/GerPS-onto/ontology#Datenfelder"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The data field ID describes the unique identifier for data fields.</rdfs:comment>
        <rdfs:label xml:lang="en">has_datafieldID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/GerPS-onto/ontology#hat_elementtyp -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/GerPS-onto/ontology#hat_elementtyp">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/GerPS-onto/ontology#Datenfelder"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:label xml:lang="en">has_element_type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/GerPS-onto/ontology#hat_frist -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/GerPS-onto/ontology#hat_frist">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Processing deadlines are tied to processes and define a period of time in which the execution of a process must be completed.</rdfs:comment>
        <rdfs:label xml:lang="en">has_deadline</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/GerPS-onto/ontology#hat_leikaID -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/GerPS-onto/ontology#hat_leikaID">
        <rdfs:subPropertyOf rdf:resource="http://BPMNbasedOntology#id"/>
        <rdfs:domain rdf:resource="https://w3id.org/GerPS-onto/ontology#Leistung"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The LeiKaID describes the unique identifier for administrative services defined by the service catalog.</rdfs:comment>
        <rdfs:label xml:lang="en">has_leikaID</rdfs:label>
    </owl:DatatypeProperty>
    


