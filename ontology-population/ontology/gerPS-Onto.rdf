    <!-- https://w3id.org/GerPS-onto/ontology#hat_typ -->

    <owl:ObjectProperty rdf:about="https://w3id.org/GerPS-onto/ontology#hat_typ">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/GerPS-onto/ontology#Prozessschritt"/>
        <rdfs:range rdf:resource="https://w3id.org/GerPS-onto/ontology#RAG"/>
